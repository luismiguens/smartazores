            <trans-unit id="pricing_description" resname="pricing_description">
                <source>pricing_description</source>
                <target>The Azores and in particular the island of São Miguel offer the same beauty and spectacular scenery throughout the year. Choose the plan for your Smart according to your personal preference!</target>
            </trans-unit>
            
            
            <trans-unit id="pricing_plan_01" resname="pricing_plan_01">
