            <trans-unit id="services_description" resname="services_description">
                <source>services_description</source>
                <target>Enquanto descobres a ilha de São Miguel nos Açores, diverte-te com o nosso Smart! Diverte-te a conduzir, a estacionar, a virar e assume uma personagem!</target>
            </trans-unit>
               
               
            <trans-unit id="services_question_01" resname="services_question_01">
